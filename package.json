  "description": "garys MEAN stack app",
  "main": "startup.js",
  "scripts": {
    "tsc": "tsc",
    "tsc-watch": "tsc -w",
    "live": "live-server --port=8080 --host=localhost",
    "api": "node-dev ./api/index.js",
    "sass": "node-sass --watch ./sass/ --output ./assets/css",
