{
  "name": "garyyochum",
  "version": "1.0.0",
  "description": "garys MEAN stack app",
  "main": "startup.js",
  "scripts": {
    "tsc": "tsc -p src -w",
    "live": "live-server --open=src",
    "api": "node-dev ./api/index.js",
    "sass": "node-sass --watch ./src/sass/ --output ./src/assets/css",
    "start": "concurrent \"npm run tsc\" \"npm run live\" \"npm run api\" \"npm run sass\" "
  },
  "keywords": [
    "angular2",
    "express",
    "responsive",
    "mongodb"
  ],
  "author": "gary yochum",
  "license": "ISC",
  "dependencies": {
    "angular2": "^2.0.0-alpha.46",
    "body-parser": "^1.14.2",
    "express": "^4.13.3",
    "mongodb": "^2.1.2",
    "mongoose": "^4.2.9",
    "systemjs": "^0.19.6"
  },
  "devDependencies": {
    "concurrently": "^1.0.0",
    "gulp": "^3.9.0",
    "gulp-sass": "^2.1.1",
    "gulp-typescript": "^2.10.0",
    "live-server": "^0.8.2",
    "typescript": "^1.6.2"
  }
}
