{	
	"compilerOptions": {
		"target": "es6",
		"module": "system",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"removeComments": false,
		"noImplicitAny": false,
		"suppressImplicitAnyIndexErrors": true,
		"noEmit": false,
		"noEmitOnError": true
	},
	"exclude": [
		"node_modules",
		"typings"
	]
}